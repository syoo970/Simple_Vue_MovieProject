{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended"
    ],
    "rules": {
        "semi": ["error","always"],
        "vue/multi-word-component-names": "off",
        "vue/attribute-hyphenation": "off"
    }
}